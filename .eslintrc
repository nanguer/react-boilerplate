{
  "extends": ["airbnb", "airbnb/hooks"],
  "plugins": ["react"],
  "parser": "babel-eslint",

  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-unused-vars": [
      "error",
      {
        "vars": "local",
        "arg": "none"
      }
    ],
    "react/jsx-filename-extension": 0
  }
}
